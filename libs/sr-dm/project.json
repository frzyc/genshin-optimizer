{
  "name": "sr-dm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/sr-dm/src",
  "projectType": "library",
  "targets": {
    "load-dm": {
      "options": {
        "repoUrl": "https://github.com/Dimbreath/StarRailData.git",
        "localPath": "libs/sr-dm/StarRailData"
      },
      "inputs": [
        {
          "runtime": "git ls-remote -q https://github.com/Dimbreath/StarRailData.git HEAD"
        }
      ],
      "outputs": ["{projectRoot}/StarRailData.hash"]
    },
    "build": {},
    "build-ts": {
      "dependsOn": ["load-dm", "^build"]
    },
    "lint": {}
  },
  "tags": [],
  "namedInputs": {
    "production": [
      "{projectRoot}/version.hash",
      "{projectRoot}/src/**/*",
      "{projectRoot}/scripts/**/*",
      "sharedGlobals",
      "{projectRoot}/.eslintrc.json",
      "{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "{projectRoot}/tsconfig.spec.json",
      "{projectRoot}/jest.config.[jt]s"
    ]
  }
}
