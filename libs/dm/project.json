{
  "name": "dm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dm/src",
  "projectType": "library",
  "targets": {
    "loadDM": {
      "executor": "@genshin-optimizer/plugin:sync-repo",
      "options": {
        "repoUrl": "https://gitlab.com/Dimbreath/AnimeGameData.git",
        "localPath": "libs/dm/GenshinData"
      },
      "outputs": ["{projectRoot}/GenshinData.hash"]
    },
    "build": {},
    "build-ts": {
      "dependsOn": ["loadDM", "^build"]
    },
    "lint": {}
  },
  "tags": [],
  "namedInputs": {
    "production": [
      "{projectRoot}/version.hash",
      "{projectRoot}/src/**/*",
      "{projectRoot}/scripts/**/*",
      "sharedGlobals",
      "{projectRoot}/.eslintrc.json",
      "{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "{projectRoot}/tsconfig.spec.json",
      "{projectRoot}/jest.config.[jt]s"
    ]
  }
}
