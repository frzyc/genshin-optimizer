{
  "homepage": "https://frzyc.github.io/genshin-optimizer/",
  "name": "genshin-optimizer",
  "version": "8.29.1",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.83",
    "@mui/material": "^5.8.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/recharts": "^1.8.23",
    "i18next": "^21.8.3",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.0",
    "pipeline": "file:./pipeline",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-ga4": "^1.4.1",
    "react-i18next": "^11.16.9",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.13",
    "source-map-explorer": "^2.5.2",
    "tesseract.js": "^2.1.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "preinstall": "node ./clone_pipeline.js && cd ./pipeline && npm install",
    "gen-files": "npm run localization-load && cd ./pipeline && npm start",
    "prestart": "npm run gen-files",
    "start": "react-scripts start",
    "prebuild": "npm run gen-files",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testserver": "npm run build && node ./test-server.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "localization-gen-main": "cd ./localization && npx ts-node ./mainGen.ts",
    "localization-load": "cd ./localization && npx ts-node ./load.ts",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "check-types": "tsc --noEmit",
    "audit": "npm audit --production",
    "pull": "git pull && cd ./pipeline && npm run pull"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-i18next": ">=11.16.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react-router-dom": "^5.3.3",
    "express": "^4.18.1",
    "gh-pages": "^3.2.3",
    "react-scripts": "^5.0.1",
    "sass": "^1.52.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
