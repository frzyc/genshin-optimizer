{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/frontend/src",
  "projectType": "application",
  "targets": {
    "build": {},
    "build-vite": {
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/frontend",
        "index": "apps/frontend/index.html",
        "main": "apps/frontend/src/main.tsx",
        "tsConfig": "apps/frontend/tsconfig.app.json",
        "assets": [
          "apps/frontend/favicon.ico",
          "apps/frontend/logo192.png",
          "apps/frontend/logo512.png",
          "apps/frontend/src/manifest.json",
          "apps/frontend/src/robots.txt",
          "apps/frontend/src/assets",
          {
            "input": "libs/gi/localization/assets",
            "glob": "**/*",
            "output": "assets"
          },
          {
            "input": "libs/gi/dm-localization/assets",
            "glob": "**/*",
            "output": "assets"
          },
          {
            "input": "libs/gi/silly-wisher-names/assets",
            "glob": "**/*",
            "output": "assets"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "frontend:build-vite"
      },
      "configurations": {
        "development": {
          "buildTarget": "frontend:build-vite:development"
        },
        "production": {
          "buildTarget": "frontend:build-vite:production"
        }
      },
      "dependsOn": ["^gen-file"]
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "frontend:build-vite"
      },
      "configurations": {
        "development": {
          "buildTarget": "frontend:build-vite:development"
        },
        "production": {
          "buildTarget": "frontend:build-vite:production"
        }
      }
    },
    "lint": {},
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "apps/frontend/vitest.config.mts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "gi-localization",
    "gi-dm-localization",
    "gi-silly-wisher-names"
  ]
}
