{
  "name": "gi-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/gi-frontend/src",
  "projectType": "application",
  "targets": {
    "build": {},
    "build-vite": {},
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "gi-frontend:build-vite"
      },
      "configurations": {
        "development": {
          "buildTarget": "gi-frontend:build-vite:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "gi-frontend:build-vite:production",
          "hmr": false
        }
      },
      "dependsOn": ["build-vite"]
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "gi-frontend:build-vite"
      },
      "configurations": {
        "development": {
          "buildTarget": "gi-frontend:build-vite:development"
        },
        "production": {
          "buildTarget": "gi-frontend:build-vite:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "apps/gi-frontend/vitest.config.ts"
      }
    },
    "lint": {},
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "gi-frontend:build-vite"
      }
    }
  },
  "tags": []
}
